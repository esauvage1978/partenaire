{% if contact.partenaires |length >0 %}
    <article class=" pt-3">
        <h5 class="text-purple p-1 ">
            Contact chez les partenaires
        </h5>
        <div class="row col-12 d-flex align-items-stretch justify-content-start " >
            {% for partenaire in contact.partenaires %}
                <div class="col-sm-12 col-md-6 d-flex align-items-stretch" >
                    {{ include('partenaire/_view/_vignette.html.twig') }}
                </div>
            {% endfor %}
        </div>
    </article>
{% endif %}